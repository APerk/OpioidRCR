LIBNAME dpath data set library;
LIBNAME opath output data set;
%MACRO summary(%variable);
	PROC MEANS DATA=dpath.dataset NOPRINT N NMISS;
		CLASS %variable;
		OUTPUT OUT=opath.%variable;
	RUN;
%MEND;
%summary(zip);
%summary(race);
%summary(sex);
%summary(hispanic);
%summary(agegrp);
%summary(eventyear);
